#include <stdio.h>
#include <conio.h>
int main()
{
    int a[100],b[100],c[100],m,n,i,j,k=0;
    printf("enter the size of array 1->");
    scanf("%d",&m);
    printf("enter the size of array 2->");
    scanf("%d",&n);
    printf("enter the elements of array 1->");
    for(i=0;i<m;i++)
    {
       scanf("%d",&a[i]);
    }
    printf("enter the elements of array 2->");
    for(j=0;j<n;j++)
    {
       scanf("%d",&b[j]);
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
           if(a[i]==b[j])
           {
               c[k]=a[i];
               k++;
           }
        }  }
    printf("the elements in intersection are");
    for(i=0;i<k;i++)
    {
    printf(" %d",c[i]);
    }
    getch();
    return 0;
}
